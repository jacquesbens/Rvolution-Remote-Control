{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": true
      }
    },
    "production-clean": {
      "extends": "production",
      "cache": {
        "disabled": true
      },
      "env": {
        "SKIP_BUNDLING": "0"
      }
    }
  },
  "submit": {
    "production": {}
  }
}